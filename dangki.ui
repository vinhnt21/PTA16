}

/* Line Edit */
QLineEdit {
    background-color: white; /* Nền trắng */
    border: 2px solid black; /* Viền đen */
    border-radius: 5px; /* Bo góc nhẹ */
    padding: 5px; /* Khoảng cách bên trong */
    color: #1a1a1a; /* Chữ màu đen */
}

QLineEdit:hover {
    border: 2px solid #4CAF50; /* Viền xanh lá khi hover */
}

QLineEdit:focus {
    border: 2px solid #4db8ff; /* Viền xanh dương khi focus */
}

/* Push Button */
QPushButton {
    background-color: #4CAF50; /* Nút màu xanh lá */
    color: white; /* Chữ trắng */
    border: 2px solid #388E3C; /* Viền nút */
    border-radius: 8px; /* Bo góc nút */
    padding: 8px 12px; /* Tăng kích thước nút */
    font-weight: bold; /* Chữ đậm */
}

QPushButton:hover {
    background-color: #45A049; /* Màu đậm hơn khi hover */
}

QPushButton:pressed {
    background-color: #357A38; /* Màu tối hơn khi bấm */
}
